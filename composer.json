{
    "name": "qobo/cakephp-groups",
    "description": "Groups plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "cakedc/users": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Groups\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Groups\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
